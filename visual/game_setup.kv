#:kivy 1.10.0

<GameSetup>:
    canvas:
        Color:
            rgb: .1, .13, .18
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        id: menus
        orientation: "vertical"
        Button:
            id: back
            text: "Back"
            on_press: app.screenmanager.current = "main_menu"
